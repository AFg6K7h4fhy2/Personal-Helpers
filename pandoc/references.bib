	file = {Garner and Hamilton - 2011 - Principles of epidemiological modelling -EN- -FR-.pdf:/Users/trevormartin/Zotero/storage/WYCBATAW/Garner and Hamilton - 2011 - Principles of epidemiological modelling -EN- -FR-.pdf:application/pdf},
