	number = {23},
	urldate = {2024-03-29},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Leech, Gavin and Rogers-Smith, Charlie and Monrad, Joshua Teperowski and Sandbrink, Jonas B. and Snodin, Benedict and Zinkov, Robert and Rader, Benjamin and Brownstein, John S. and Gal, Yarin and Bhatt, Samir and Sharma, Mrinank and Mindermann, Sören and Brauner, Jan M. and Aitchison, Laurence},
